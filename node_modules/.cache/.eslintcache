bleWarningCount":0,"source":"115"},{"filePath":"218","messages":"219","suppressedMessages":"220","errorCount":0,"fatalErrorCount":0,"warningCount":3,"fixableErrorCount":0,"fixableWarningCount":0,"source":"174"},{"filePath":"221","messages":"222","suppressedMessages":"223","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/index.tsx",[],[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/App.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/components/ItemDetail.tsx",["224"],[],"import {  Backdrop, Button, CircularProgress } from \"@mui/material\";\nimport React, { useEffect } from \"react\";\nimport { AppDispatch, useSelector } from \"../store\";\nimport {  useParams } from \"react-router-dom\";\nimport { getDetail } from \"../redux/item/itemSlice\";\nimport { useDispatch } from \"react-redux\";\nimport { addCart } from \"../redux/cart/cartSlice\";\n\n\nexport const ItemDetail = () => {\n  // type apiDetailArray = {\n  //     id: number;\n  //     title: string;\n  //     image: string;\n  //     price: number;\n  //     category: string;\n  //     description: string;\n  // };\n\n  const dispatch = useDispatch<AppDispatch>();\n  const {detail}:any= useSelector((store) => store.items);\n  const { productId } = useParams();\n  const open:boolean = true;\n\n  useEffect(() => {\n    // イベントの設定\n    window.addEventListener('beforeunload', onUnload);\n    return () => {\n      // イベントの設定解除\n      window.removeEventListener('beforeunload', onUnload);\n    }\n  })\n  const onUnload = (e: { preventDefault: () => void; returnValue: string; }) => {\n    e.preventDefault();\n    e.returnValue = '';\n  }\n\n  useEffect(() => {\n    dispatch(getDetail(productId));\n  });\n\n\n  return (\n    <div className=\"container\">\n\n      \n      {detail ? (\n        <div>\n          {\n            <div className=\"detail flex\" key={detail.id}>\n              <div className=\"detail__image\">\n                <img src={detail.image} />\n              </div>\n              <div className=\"detail__title\">\n                <h1 className=\"detail__title__text\">{detail.title}</h1>\n                <h2 className=\"detail__title__text\">\n                  {detail.price * 100}円\n                </h2>\n                <h3 className=\"detail__title__text\">カテゴリー：{detail.category}</h3>\n                <p>{detail.description}</p>\n                <div className=\"detail__title__text\">\n                  <Button variant=\"contained\" onClick={() => {dispatch(addCart(detail)) }} className=\"detail__cart_btn\">\n                    カートに入れる\n                  </Button>\n                </div>\n              </div>\n            </div>\n          }\n        </div>\n      ) : (\n        <Backdrop open={open}>\n        <CircularProgress color=\"inherit\" />\n      </Backdrop>\n      )}\n    </div>\n  );\n};\n","/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/components/ItemsLists.tsx",["225","226"],[],"import React, { useEffect, useState } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { useSearchParams } from \"react-router-dom\";\nimport Item from \"./Item\";\nimport { Sidebar } from \"./Sidebar\";\nimport { getItems } from \"../redux/item/itemSlice\";\nimport { AppDispatch, useSelector } from \"../store\";\n\nexport const ItemsLists = () => {\n  type apiArray = {\n    id: number;\n    title: string;\n    price: number;\n    category: string;\n    image: string;\n  };\n\n  const dispatch = useDispatch<AppDispatch>();\n  const {items, maxPriceRange, minPriceRange} = useSelector((store) => store.items)\n  const [filterItem, setFilterItem] = useState<apiArray[]>([]);\n  const [filterFlag, setFilterFlag] = useState<boolean>(false);\n  const [searchParams] = useSearchParams();\n  const categoryParams = searchParams.get(\"category\");\n\n\n\n  // API反映\n  useEffect(() => {\n    dispatch(getItems());\n  }, []);\n\n\n  // 絞り込み\n  useEffect(() => {\n    let copyPosts = [...items];\n\n    // 料金絞り込み\n    if(!minPriceRange && maxPriceRange === 100000){\n      setFilterFlag(false);\n    } else if(minPriceRange || maxPriceRange){\n      copyPosts = copyPosts.filter((post: apiArray) => {\n       return (post.price < maxPriceRange/100 && post.price > minPriceRange/100)\n      })\n      setFilterItem(copyPosts);\n      setFilterFlag(true);\n\n    }\n\n    // カテゴリー絞り込み\n    if (categoryParams === \"all\") {\n      setFilterItem(copyPosts);\n      return;\n    } else if (categoryParams) {\n      copyPosts = copyPosts.filter((post: apiArray) =>\n        post.category.match(categoryParams)\n      );\n      setFilterFlag(true);\n    }\n    setFilterItem(copyPosts);\n  }, [categoryParams ,maxPriceRange, minPriceRange]);\n\n  return (\n    <div className=\"container item-lists-wrap flex\">\n      <Sidebar />\n      {items ? (\n        <div className=\"item-lists flex\">\n          {filterFlag\n            ? filterItem.map((item: apiArray) => {\n                return <Item key={item.id} {...item} />;\n              })\n            : items.map((item: apiArray) => {\n                return <Item key={item.id} {...item} />;\n              })}\n        </div>\n      ) : (\n\n        <div>Loading...</div>\n      )}\n    </div>\n  );\n};\n","/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/components/Header.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/components/Item.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/redux/item/itemSlice.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/components/Sidebar.tsx",["227","228"],[],"import { Button } from \"@mui/material\";\nimport React, { useEffect } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { useSelector } from \"../store\";\nimport {\n  clickPrice,\n  handleMaxPrice,\n  handleMinPrice,\n} from \"../redux/item/itemSlice\";\nimport {\n  createSearchParams,\n  useNavigate,\n  useSearchParams,\n} from \"react-router-dom\";\nimport CategoryLists from \"../categoryLists\";\n\nexport const Sidebar = () => {\n  const navigate = useNavigate();\n  const dispatch = useDispatch();\n  const { maxPrice, minPrice } = useSelector((state) => state.items);\n  const [searchParams, setSearchParams] = useSearchParams();\n  let params = \"\";\n\n  // カテゴリ絞り込み\n  const handleChecked = (e: React.ChangeEvent<HTMLInputElement>) => {\n    let checkedId = e.target.id;\n    let checkedName = e.target.value;\n    params = createSearchParams({\n      categoryId: checkedId,\n      category: checkedName,\n    }).toString();\n    navigate(`?${params}`, { replace: false });\n  };\n\n  useEffect(() => {\n    setSearchParams(params);\n  }, [params]);\n\n  return (\n    <div className=\"sidebar\">\n      <div className=\"price-wrap\">\n        <p>値段で絞り込む</p>\n        <div className=\"price-wrap__multithumb\">\n          <input\n            className=\"price-wrap__thumb1\"\n            type=\"range\"\n            name=\"range-1\"\n            id=\"back\"\n            onChange={(e) => dispatch(handleMinPrice(e.target.value))}\n            value={minPrice}\n            min=\"0\"\n            max=\"100000\"\n            step=\"1000\"\n          />\n          <input\n            className=\"price-wrap__thumb2\"\n            type=\"range\"\n            name=\"range-2\"\n            id=\"front\"\n            onChange={(e) => dispatch(handleMaxPrice(e.target.value))}\n            value={maxPrice}\n            min=\"0\"\n            max=\"100000\"\n            step=\"1000\"\n          />\n        </div>\n\n        <p>\n          {minPrice}円〜{maxPrice}円\n        </p>\n        <Button variant=\"outlined\" onClick={() => dispatch(clickPrice())}>\n          料金絞り込み\n        </Button>\n      </div>\n      <div className=\"cate-wrap\">\n        <p>カテゴリー一覧</p>\n        <ul>\n          {/* カテゴリ表示 */}\n          {CategoryLists.map((categoryList) => (\n            <li key={categoryList.id}>\n              <label htmlFor={categoryList.id}>\n                <input\n                  type=\"radio\"\n                  name=\"category\"\n                  id={categoryList.id}\n                  value={categoryList.category}\n                  onChange={(e) => handleChecked(e)}\n                />\n                {categoryList.categoryName}\n              </label>\n            </li>\n          ))}\n        </ul>\n      </div>\n    </div>\n  );\n};\n\n// https://www.hypertextcandy.com/react-tutorial-04-form-and-events\n","/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/store.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/index.tsx",[],[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/store.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/App.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/components/Header.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/components/ItemDetail.tsx",["229","230","231"],[],"import { Backdrop, Button, CircularProgress } from \"@mui/material\";\nimport React, { useEffect } from \"react\";\nimport { AppDispatch, useSelector } from \"../store\";\nimport { useParams } from \"react-router-dom\";\nimport { getDetail } from \"../redux/item/itemSlice\";\nimport { useDispatch } from \"react-redux\";\nimport { addCart } from \"../redux/cart/cartSlice\";\n\n\nexport const ItemDetail = () => {\n  type apiDetailArray = {\n      id: number;\n      title: string;\n      image: string;\n      price: number;\n      category: string;\n      description: string;\n  };\n\n\n  const dispatch = useDispatch<AppDispatch>();\n  const {detail}:any= useSelector((state) => state.items);\n  const { productId } = useParams();\n  const open:boolean = true;\n\n\n\n\n  useEffect(() => {\n    dispatch(getDetail(productId));\n    \n  }, []);\n\n\n  return (\n    <div className=\"container\">\n      \n      {detail ? (\n        <div>\n          {\n            <div className=\"detail flex\" key={detail.id}>\n              <div className=\"detail__image\">\n                <img src={detail.image} />\n              </div>\n              <div className=\"detail__title\">\n                <h1 className=\"detail__title__text\">{detail.title}</h1>\n                <h2 className=\"detail__title__text\">\n                  {detail.price * 100}円\n                </h2>\n                <h3 className=\"detail__title__text\">カテゴリー：{detail.category}</h3>\n                <p>{detail.description}</p>\n                <div className=\"detail__title__text\">\n                  <Button variant=\"contained\" onClick={() => {dispatch(addCart(detail))}} className=\"detail__cart_btn\">\n                    カートに入れる\n                  </Button>\n                </div>\n              </div>\n            </div>\n          }\n        </div>\n      ) : (\n        <Backdrop open={open}>\n        <CircularProgress color=\"inherit\" />\n      </Backdrop>\n      )}\n    </div>\n  );\n};\n","/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/components/ItemsLists.tsx",["232"],[],"import React, { useEffect, useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useSearchParams } from \"react-router-dom\";\nimport Item from \"./Item\";\nimport { Sidebar } from \"./Sidebar\";\nimport { getItems } from \"../redux/item/itemSlice\";\nimport { AppDispatch } from \"../store\";\n\nexport const ItemsLists = () => {\n  type apiArray = {\n    id: number;\n    title: string;\n    price: number;\n    category: string;\n    image: string;\n  };\n\n  const dispatch = useDispatch<AppDispatch>();\n  const {items, maxPriceRange, minPriceRange} = useSelector((state:any) => state.items)\n  const [filterItem, setFilterItem] = useState<apiArray[]>([]);\n  const [filterFlag, setFilterFlag] = useState<boolean>(false);\n  const [searchParams] = useSearchParams();\n  const categoryParams = searchParams.get(\"category\");\n\n\n\n  // API反映\n  useEffect(() => {\n    dispatch(getItems());\n  }, [dispatch]);\n\n\n  // 絞り込み\n  useEffect(() => {\n    let copyPosts = [...items];\n\n    // 料金絞り込み\n    if(!minPriceRange && maxPriceRange === 100000){\n      setFilterFlag(false);\n    } else if(minPriceRange || maxPriceRange){\n      copyPosts = copyPosts.filter((post: apiArray) => {\n       return (post.price < maxPriceRange/100 && post.price > minPriceRange/100)\n      })\n      setFilterItem(copyPosts);\n      setFilterFlag(true);\n\n    }\n\n    // カテゴリー絞り込み\n    if (categoryParams === \"all\") {\n      setFilterItem(copyPosts);\n      return;\n    } else if (categoryParams) {\n      copyPosts = copyPosts.filter((post: apiArray) =>\n        post.category.match(categoryParams)\n      );\n      setFilterFlag(true);\n    }\n    setFilterItem(copyPosts);\n  }, [categoryParams ,maxPriceRange, minPriceRange]);\n\n  return (\n    <div className=\"container item-lists-wrap flex\">\n      <Sidebar />\n      {items ? (\n        <div className=\"item-lists flex\">\n          {filterFlag\n            ? filterItem.map((item: apiArray) => {\n                return <Item key={item.id} {...item} />;\n              })\n            : items.map((item: apiArray) => {\n                return <Item key={item.id} {...item} />;\n              })}\n        </div>\n      ) : (\n\n        <div>Loading...</div>\n      )}\n    </div>\n  );\n};\n","/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/redux/item/itemSlice.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/components/Sidebar.tsx",["233","234"],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/components/Item.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-siteのコピー/src/categoryLists.tsx",[],[],"/Users/yonekawataka