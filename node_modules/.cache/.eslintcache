artItems.map((item: cartArray) => {\n              return <CartItem key={item.id} {...item} />;\n            })\n          : \"\"} */}\n      </div>\n      <footer>\n        <hr />\n        <div className=\"cart-total\">\n          <h4>\n            合計 <span>{Math.floor(totalPrice * 100)}円</span>\n          </h4>\n        </div>\n        <div className=\"clear-btn\">\n          <Button className=\"btn \" onClick={() => dispatch(removeAll())}>\n            全削除\n          </Button>\n        </div>\n      </footer>\n    </section>\n  );\n}\n\nexport default CartContainer;\n","/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/redux/cart/cartSlice.tsx",[],[],"/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/components/cart/CartItem.tsx",["237","238","239"],[],"import React, { useEffect } from \"react\";\nimport { Checkbox } from \"@mui/material\";\nimport { useDispatch } from \"react-redux\";\nimport { AppDispatch, useSelector } from \"../../store\";\nimport { removeItem } from \"../../redux/cart/cartSlice\";\n\n\ntype apiArray = {\n  id: number | string;\n  title: string;\n  price: number;\n  image: string;\n};\n\nfunction CartItem({ id, image, price, title }: apiArray) {\n  const dispatch = useDispatch<AppDispatch>();\n  const { cartItems , cartShow } = useSelector((store) => store.cart);\n\n  // 要素数を数える\n  let count = cartItems.filter(function(x){return x.id===id}).length;\n\n\n\n  return (\n    <article className=\"cart-item\">\n      <img src={image} alt={title} className=\"cart-item__image\" />\n      <div>\n        <h4>{title}</h4>\n        <h4 className=\"item-price\">{price * 100}円</h4>\n        <button className=\"remove-btn\" onClick={() => dispatch(removeItem(id))}>\n          削除\n        </button>\n        <p>注文数：{count}</p>\n      </div>\n      <div></div>\n    </article>\n  );\n}\n\nexport default CartItem;\n","/Users/yonekawatakashinari/Desktop/js-practice/yone-ec-site/src/categoryLists.tsx",[],[],"/Users/yonekawatakashinari/D